try {
  $uruWrapperBat = Join-Path $nugetExePath "<%= data[:name] %>.bat"
  $uruWrapperPs1 = Join-Path $nugetExePath "<%= data[:name] %>.ps1"

  Write-Host "---> Deleting .bat and .ps1 uru wrapper scripts in $nugetExePath"
  if (Test-Path ($uruWrapperBat)) { Remove-Item $uruWrapperBat -force }
  if (Test-Path ($uruWrapperPs1)) { Remove-Item $uruWrapperPs1 -force }

} catch {
  Write-ChocolateyFailure "$packageName" "$($_.Exception.Message)"
  throw
}
